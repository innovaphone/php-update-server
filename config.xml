    <nobootdev> 
        <model>ipva</model>
        <model>ipvadec</model>
        <model>swphone</model>
        <model>mypbxa</model>
        <model>mypbxi</model>
    </nobootdev>
    
    <nofirmdev>
        <model>swphone</model>
        <model>mypbxi</model>
    </nofirmdev>
    
    <stdargs>
        <stdarg key="sn" value="#m"/>
        <stdarg key="type" value="#t"/>
        <stdarg key="hwid" value="#h"/>
        <stdarg key="ip" value="#i"/>
        <stdarg key="PROT"/>
        <stdarg key="PROTdate"/>
        <stdarg key="BOOT"/>
        <stdarg key="BOOTdate"/>        
        <stdarg key="BMC"/>
        <stdarg key="BMCdate"/>
        <stdarg key="CFG"/>
        <stdarg key="CFGdate"/>
        <stdarg key="SCFG"/>
        <stdarg key="SCFGdate"/>
        <stdarg key="CHECK"/>
        <stdarg key="CHECKdate"/>
        <stdarg key="CFG-BACKUP-NUM"/>
        <stdarg key="CFG-BACKUP-NUMdate"/>
        <stdarg key="ver"/>
    </stdargs>
    
    
    <customcerts
        CAkeys="CAkey*"
        CAtype="manual"
        CAname="innovaphone Device Certification Authority,innovaphone Device Certification Authority 2"
        CAnamesep=","
        
        CSRkey="2048-bit"
        CSRsignature="SH256"
        CSRdn-cn="{sn}"

        renew="90"
        storekey="true"
    />
    
    <queries
        scfg="update.php?mode=query&amp;sn=#m"
    >
        <query id="certificates" title="Certificate">
            <!-- get info for custom certificates -->
            <cmd>mod cmd X509 xml-info</cmd>
            <!-- <applies>*</applies> -->
        </query>
        <query id="admin" title="Admin">
            <cmd>mod cmd CMD0 xml-info</cmd>
            <!-- <applies>*</applies> -->
        </query>
    </queries>
</config>

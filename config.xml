<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE config SYSTEM "config.dtd">

<!--
This file contains all the default values.
Do NOT modify this file!   Put local settings in to user-config.xml rather
-->

<config debugmerge="false" debugcerts="false" debugscript="false">
    
    <master 
        info="http://yourmasterdevice.yourdomain.tld/CMD0/box_info.xml"
        expire="3600"
        cache="cache/master-info.xml"
        user="admin"
        password="password"
    />
    
    <fwstorage url="fw/{build}/"/>
    <backup 
        nbackups="10"
        scfg="update.php?mode=backup&amp;hwid=#h&amp;sn=#m"
    />
    <status
        missing="604800" 
        refresh="0"
        expire="7776000" 
        logkeep="1800"
        usehttpsdevlinks="true"
    />
    <times 
        dir="scripts"
        check="true"
        polling="5"
        interval="15"
        grace="90"
        forcehttps=""
        httpsport=""
        forcetrust=""
        httpsurlmod="!mtls/!!i"
    />
    
    <phases>
        <!-- sequence is important! -->
        <phase id="update" seq="200"/>
    </phases>
    
    <environments>
        <environment id='default'/>
    </environments>
    <classes>
        <class id="phone">
            <!-- all phone types -->
            <model>ip110</model>
            <model>ip110A</model>
            <model>ip111</model>
            <model>ip112</model>
            <model>ip150</model>
            <model>ip150A</model>
            <model>ip152</model>
            <model>ip153</model>
            <model>ip154</model>
            <model>ip200A</model>

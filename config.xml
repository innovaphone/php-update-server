            <model>ip230A</model>
            <model>ip232</model>
            <model>ip240</model>
            <model>ip240A</model>
            <model>ip241</model>
            <model>ip242</model>
            <model>ip72</model>
            <model>swphone</model>
            <model>mypbxi</model>
            <model>mypbxa</model>
        </class>
        <class id="opus_phone">
            <!-- all phones that support opus -->
            <model>ip111</model>
            <model>ip112</model>
            <model>mypbxi</model>
            <model>mypbxa</model>
